  },
  "keywords": [
    "prompt",
    "template",
    "variable",
    "expander",
    "AI",
    "dynamic"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "script.js"
    ]
  }
}